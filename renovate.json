{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "configMigration": true,
  "extends": [
    "config:recommended",
    ":prConcurrentLimitNone",
    ":prHourlyLimitNone",
    ":automergePatch",
    ":automergeBranch"
  ],
  "ignoreTests": true,
  "packageRules": [
    {
      "matchPackagePatterns": ["^lscr.io/linuxserver/"],
      "versioning": "regex:^(?<build>.*)-v?(?<major>\\d+)\\.(?<minor>\\d+)\\.(?<patch>[\\d.]+)?(?:-(?<release>.*))?$"
    },
    {
      "matchFileNames": ["media/compose.yaml"],
      "matchPackageNames": ["rclone/rclone"],
      "enabled": false
    }
  ]
}
